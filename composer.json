{
    "name": "outhebox/laravel-translations",
    "description": "Manage your Laravel translations with a beautiful UI. Add, edit, delete, import, and export translations with ease.",
    "keywords": [
        "laravel",
        "laravel-translations",
        "translations",
        "localization",
        "inertiajs",
        "inertia",
        "translation-manager",
        "i18n"
    ],
    "homepage": "https://github.com/MohmmedAshraf/laravel-translations",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Ashraf",
            "email": "cupo.ashraf@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0||^12.0",
        "illuminate/support": "^11.0||^12.0",
        "inertiajs/inertia-laravel": "^2.0",
        "spatie/laravel-query-builder": "^6.0||^7.0"
    },
    "require-dev": {
        "laravel/boost": "^2.1",
        "laravel/pint": "^1.0",
        "laravel/wayfinder": "^0.1",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0||^10.0",
        "orchestra/workbench": "^9.0||^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Outhebox\\Translations\\": "src/",
            "Outhebox\\Translations\\Database\\Factories\\": "database/factories/",
            "Outhebox\\Translations\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Outhebox\\Translations\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare",
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/pint --ansi"
        ],
        "generate-routes": "@php vendor/bin/testbench wayfinder:generate --path=resources/js --with-form --ansi"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Outhebox\\Translations\\Providers\\TranslationsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
